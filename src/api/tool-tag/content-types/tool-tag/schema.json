{
  "kind": "collectionType",
  "collectionName": "tool_tags",
  "info": {
    "singularName": "tool-tag",
    "pluralName": "tool-tags",
    "displayName": "Tool Tag",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 20,
      "required": true
    },
    "color": {
      "type": "customField",
      "customField": "plugin::color-picker.color",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$"
    },
    "aiTools": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::ai-tool.ai-tool",
      "mappedBy": "toolTags"
    }
  }
}
